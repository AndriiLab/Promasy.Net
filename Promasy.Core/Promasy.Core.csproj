        <Nullable>enable</Nullable>
        <LangVersion>default</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <EmbeddedResource Update="Resources\Resources\SharedResource.uk.resx">
        <Generator>ResXFileCodeGenerator</Generator>
